{
  "name": "platform-extension-starter",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "tslint": "tslint",
    "start": "npm run start:node",
    "start:dev": "npm-run-all -p -r start:ng start:node",
    "start:aot": "npm-run-all -p -r start:ng:aot start:node",
    "start:node": "nodemon server/index.js",
    "start:ng": "npm run ng -- build --aot=false --watch",
    "start:ng:aot": "npm run ng -- build --aot --watch",
    "analyze": "npm run ng -- build --verbose --json --aot --prod",
    "lint": "npm run tslint -- \"public/**/*.ts\" --format verbose --type-check --project tsconfig.json",
    "build": "npm run ng -- build --aot --prod",
    "deploy": "gcloud app deploy"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.0.0",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "body-parser": "^1.17.2",
    "core-js": "^2.4.1",
    "ejs": "^2.5.5",
    "express": "^4.14.1",
    "hint.css": "^2.5.0",
    "nodemon": "^1.11.0",
    "novo-elements": "^2.1.50",
    "npm-run-all": "^4.0.1",
    "rxjs": "^5.1.0",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "^1.3.1",
    "@angular/compiler-cli": "^4.0.0",
    "@angular/language-service": "^4.0.0",
    "@types/node": "~6.0.60",
    "codelyzer": "~3.1.1",
    "ts-node": "~3.0.4",
    "tslint": "~5.3.2",
    "typescript": "~2.3.3"
  }
}
